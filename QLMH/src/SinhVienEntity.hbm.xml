<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hibernate.SinhVienEntity" table="SinhVien" schema="DB_QLMH">
        <id name="mssv">
            <column name="MSSV" sql-type="char(8)" length="8"/>
        </id>
        <property name="hoTen">
            <column name="HoTen" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="gioiTinh">
            <column name="GioiTinh" sql-type="char(3)" length="3" not-null="true"/>
        </property>
        <property name="cmnd">
            <column name="CMND" sql-type="char(15)" length="15" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>